{
  "name": "tox-ccc-ui",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "author": "Enrico Hoffmann <dasrick@gmail.com>",
  "license": "propertieray",
  "engines": {
    "node": "0.10.*",
    "npm": "2.2.*"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "browserify-ngannotate"
    ]
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js"
  },
  "browserify-shim": {
    "angular": "angular"
  },
  "dependencies": {
    "angular": "~1.3.9",
    "browserify": "^8.0.0",
    "browserify-ngannotate": "^0.6.0",
    "browserify-shim": "^3.8.0",
    "clean-css": "^3.0.7",
    "express": "^4.11.2",
    "less": "^2.2.0",
    "less-plugin-autoprefix": "^1.3.0",
    "mi24-bootstrap": "^0.1.6",
    "parallelshell": "^1.0.3"
  },
  "devDependencies": {
    "david": "^6.1.1"
  },
  "scripts": {
    "postinstall": "npm run build",

    "start": "node serve.js",

    "build": "parallelshell 'npm run build:scripts' 'npm run build:css'",
    "build:scripts": "browserify src/app.js -o web/js/app.js",
    "build:css": "lessc --autoprefix='last 2 versions,ie 9' src/media/less/style.less web/media/css/style.css && cleancss --source-map -o web/media/css/style.min.css web/media/css/style.css"
  }
}
